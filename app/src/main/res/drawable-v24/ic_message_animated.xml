<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:name="path"
                android:pathData="M 4 4 L 20 4 L 20 16 L 5.17 16 L 4 17.17 L 4 4 M 4 2 C 2.9 2 2.01 2.9 2.01 4 L 2 22 L 6 18 L 20 18 C 21.1 18 22 17.1 22 16 L 22 4 C 22 2.9 21.1 2 20 2 L 4 2 Z"
                android:fillColor="#FF000000"/>
            <group android:name="group">
                <path
                    android:name="path_1"
                    android:pathData="M 6 6 L 18 6 L 18 8 L 6 8 L 6 6 Z"
                    android:fillColor="#FF000000"/>
            </group>
            <group android:name="group_1">
                <path
                    android:name="path_2"
                    android:pathData="M 6 9 L 18 9 L 18 11 L 6 11 L 6 9 Z"
                    android:fillColor="#FF000000"/>
            </group>
            <group android:name="group_2">
                <path
                    android:name="path_3"
                    android:pathData="M 6 12 L 18 12 L 18 14 L 6 14 L 6 12 Z"
                    android:fillColor="#FF000000"/>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:duration="300"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/overshoot"/>
                <objectAnimator
                    android:propertyName="pivotX"
                    android:duration="300"
                    android:valueFrom="6"
                    android:valueTo="6"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/overshoot"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_1">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="100"
                    android:duration="310"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/overshoot"/>
                <objectAnimator
                    android:propertyName="pivotX"
                    android:startOffset="100"
                    android:duration="310"
                    android:valueFrom="6"
                    android:valueTo="6"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/overshoot"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_2">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="scaleX"
                    android:startOffset="200"
                    android:duration="300"
                    android:valueFrom="0"
                    android:valueTo="1"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/overshoot"/>
                <objectAnimator
                    android:propertyName="pivotX"
                    android:startOffset="200"
                    android:duration="300"
                    android:valueFrom="6"
                    android:valueTo="6"
                    android:valueType="floatType"
                    android:interpolator="@android:interpolator/overshoot"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
